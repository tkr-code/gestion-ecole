{% extends 'admin/base.html.twig' %}
{% import "macros/links.html.twig" as links %}
{% import "macros/buttons.html.twig" as buttons %}

{% block title %}
    Edit ResponsableDepartement
        
    
{% endblock %}

{% block body %}
    <div class="card card-outline card-success">
        <div class="card-header">
            <h3>
                Edit ResponsableDepartement
                                            
                            
            </h3>
        </div>
        <div class="card-body">
            {{ form_start(form) }}
            {{ form_row(form.date_entre_fonction) }}
            {{ form_row(form.date_sortie_fonction) }}
            {{ form_row(form.departement) }}
            <div class="btn-group">
                {{ links.back(path('admin_responsable_departement_index')) }}
                {{ buttons.edit() }}
            </div>
            {{ form_end(form) }}

        </div>

    </div>

{% endblock %}

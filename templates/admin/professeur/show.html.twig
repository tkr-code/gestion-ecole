{% extends 'admin/base.html.twig' %}
{% import "macros/links.html.twig" as links %}

{% block title %}
    Professeur
                
            
        
    
{% endblock %}

{% block body %}
    <div class="card card-outline card-primary">
        <div class="card-header">
            <h3>
                Professeur              
                                            
                            
            </h3>
        </div>
        <div class="card-body">
            <table class="table">
                <tbody>
                    <tr>
                        <th>
                            Id
                                                                                                                            
                                                                                                        
                                                                                
                                                    
                        </th>
                        <td>
                            {{ professeur.id }}
                        </td>
                    </tr>
                    <tr>
                        <th>
                            Titre
                                                                                                                            
                                                                                                        
                                                                                
                                                    
                        </th>
                        <td>
                            {{ professeur.titre }}
                        </td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
    <div class="btn-group">
        {{ links.back(path('admin_professeur_index')) }}
        {{ links.edit(path('admin_professeur_edit', {'id': professeur.id})) }}
    </div>
{% endblock %}

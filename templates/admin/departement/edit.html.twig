{% import "macros/button.html.twig" as buttons %}
{% import "macros/link.html.twig" as links %}
{% extends 'admin/base.html.twig' %}

{% block title %}Edit Departement
{% endblock %}

{% block body %}

	<div class="card card-outline card-success">
		<div class="card-header">
			<h3>Edit Departement</h3>
		</div>
		<div class="card-body">
			{{form_start(form)}}
			{{form_row(form.code)}}
			{{form_row(form.designation)}}
			<div class="btn">
				{{ links.back(path('admin_departement_index'))}}
				{{buttons.edit()}}
			</div>
			{{form_end(form)}}
		</div>
	</div>
	{#    	<a href="{{ path('admin_departement_index') }}">back to list</a>
			#}
{% endblock %}
